<%# resource_cache(resource) do %>
  <article class="<%= resource_instance_name %>">
      <%= render_page_title(resource.title) %>
      <%= render_slideshow_for(resource) %>
      <%= render_html_text(resource.body) %>
    </article>
    <% @rental_properties.each do |property| %>
        <%= property.title %>
    <% end -%>
<%# end %>
